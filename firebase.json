{
  "hosting": {
    "public": "dist/redesigned/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/ru/**",
        "destination": "/ru/index.html"
      },
      {
        "source": "/en/**",
        "destination": "/en/index.html"
      },
      {
        "source": "/ua/**",
        "destination": "/ua/index.html"
      }
    ],
    "redirects" : [
      {
        "source" : "/",
        "destination" : "/ru",
        "type" : 301
      }
    ]
  }
}
